  807:  /******************************************************************************
  808:  * The AppPollWaitTimeout() function will check if it is time to send out an
  809:  * MLME-Poll request in order to receive data from the coordinator. If its time,
  810:  * and we are permitted then a poll request is created and sent.
  811:  * 
  812:  * The function uses the coordinator information gained during the Active Scan
  813:  * for building the MLME-Poll Request message. The message is sent to the MLME
  814:  * service access point in the MAC.
  815:  ******************************************************************************/
  816:  static void    AppPollWaitTimeout(uint8_t tmr)
  817:  { 
  0000 87       [2]             PSHA  
  0001 a7fe     [2]             AIS   #-2
  818:    
  819:    /* Just to avoid the compiler warning */
  820:    tmr++;
  0003 95       [2]             TSX   
  0004 6c02     [5]             INC   2,X
  821:    
  822:    /* Check if we are permitted, and if it is time to send a poll request.
  823:       The poll interval is adjusted dynamically to the current band-width
  824:       requirements. */
  825:    if(mWaitPollConfirm == FALSE)
  0006 c60000   [4]             LDA   mWaitPollConfirm
  0009 2648     [3]             BNE   L53 ;abs = 0053
  826:    {
  827:      /* This is an MLME-POLL.req command. */
  828:      mlmeMessage_t *pMlmeMsg = MSG_AllocType(mlmeMessage_t);
  000b a623     [2]             LDA   #35
  000d ac000000 [8]             CALL  MM_Alloc
  0011 9eff01   [5]             STHX  1,SP
  829:      if(pMlmeMsg)
  0014 273d     [3]             BEQ   L53 ;abs = 0053
  830:      {
  831:        /* Create the Poll Request message data. */
  832:        pMlmeMsg->msgType = gMlmePollReq_c;
  0016 9efe01   [5]             LDHX  1,SP
  0019 a60c     [2]             LDA   #12
  001b f7       [2]             STA   ,X
  833:   
  834:        /* Use the coordinator information we got from the Active Scan. */
  835:        FLib_MemCpy(pMlmeMsg->msgData.pollReq.coordAddress, mCoordInfo.coordAddress, 8);
  001c af01     [2]             AIX   #1
  001e 89       [2]             PSHX  
  001f 8b       [2]             PSHH  
  0020 450000   [3]             LDHX  @mCoordInfo
  0023 a608     [2]             LDA   #8
  0025 ac000000 [8]             CALL  FLib_MemCpy
  0029 a702     [2]             AIS   #2
  836:        FLib_MemCpy(pMlmeMsg->msgData.pollReq.coordPanId, mCoordInfo.coordPanId, 2);
  002b 9efe01   [5]             LDHX  1,SP
  002e af09     [2]             AIX   #9
  0030 89       [2]             PSHX  
  0031 8b       [2]             PSHH  
  0032 450008   [3]             LDHX  @mCoordInfo:8
  0035 a602     [2]             LDA   #2
  0037 ac000000 [8]             CALL  FLib_MemCpy
  003b a702     [2]             AIS   #2
  837:        pMlmeMsg->msgData.pollReq.coordAddrMode = mCoordInfo.coordAddrMode;
  003d 9efe01   [5]             LDHX  1,SP
  0040 c6000a   [4]             LDA   mCoordInfo:10
  0043 e70b     [3]             STA   11,X
  838:  #ifndef gMAC2006_d      
  839:        pMlmeMsg->msgData.pollReq.securityEnable = FALSE;
  840:  #else
  841:        pMlmeMsg->msgData.pollReq.securityLevel = 0;
  0045 6f0c     [5]             CLR   12,X
  842:  #endif //gMAC2006_d      
  843:        
  844:        /* Send the Poll Request to the MLME. */
  845:        if(MSG_Send(NWK_MLME, pMlmeMsg) == gSuccess_c)
  0047 ac000000 [8]             CALL  NWK_MLME_SapHandler
  004b 4d       [1]             TSTA  
  004c 2605     [3]             BNE   L53 ;abs = 0053
  846:        {
  847:          /* Do not allow another Poll request before the confirm is received. */
  848:          mWaitPollConfirm = TRUE;
  004e a601     [2]             LDA   #1
  0050 c70000   [4]             STA   mWaitPollConfirm
  0053          L53:    
  849:  
  850:        }
  851:      }
  852:    }
  853:   /* Restart timer. */
  854:   TMR_StartSingleShotTimer(mTimer_c, mPollInterval, AppPollWaitTimeout);
  0053 c60000   [4]             LDA   mTimer_c
  0056 87       [2]             PSHA  
  0057 320000   [5]             LDHX  mPollInterval
  005a 89       [2]             PSHX  
  005b 8b       [2]             PSHH  
  005c 4f       [1]             CLRA  
  005d 87       [2]             PSHA  
  005e 87       [2]             PSHA  
  005f 450000   [3]             LDHX  @AppPollWaitTimeout
  0062 89       [2]             PSHX  
  0063 8b       [2]             PSHH  
  0064 a600     [2]             LDA   @AppPollWaitTimeout:PAGE
  0066 87       [2]             PSHA  
  0067 ac000000 [8]             CALL  TMR_StartSingleShotTimer
  855:  }
